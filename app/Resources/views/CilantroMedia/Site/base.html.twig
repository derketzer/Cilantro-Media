<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Cilantro Media{% endblock %}</title>
        <!---Custom CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/custom.css') }}" type="text/css">
        <!---BootStrap CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/bootstrap.css') }}" type="text/css">
        <!---Menu CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/mega-menu.css') }}" type="text/css">
        <!---Theme Color CSS-->
        <link rel="stylesheet" href="{{ asset(themePath) }}" type="text/css">
        <!---Responsive CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/responsive.css') }}" type="text/css">
        <!---Owl Slider CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/owl.carousel.css') }}" type="text/css">
        <!---BxSlider CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/jquery.bxslider.css') }}" type="text/css">
        <!---Font Awesome CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/font-awesome.min.css') }}" type="text/css">
        <!---PrettyPhoto CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/prettyPhoto.css') }}" type="text/css">
        <!---Audio Player CSS-->
        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/site-assets/css/audioplayer.css') }}" type="text/css">

        <link rel="stylesheet" href="{{ asset('bundles/cilantromedia/css/site/default.css') }}" type="text/css">

        <!---Font Family Roboto CSS-->
        <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900' rel='stylesheet' type='text/css'>
    </head>
    <body>
        <!--Wrapper Content Start-->
        <div id="wrapper">
            {% include('CilantroMedia/Site/Includes/header.html.twig') %}

            {% block body %}{% endblock %}

            {% include('CilantroMedia/Site/Includes/footer.html.twig') %}

        </div>
        <!--Wrapper Content End-->

        <!---JQuery-1.11.3.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/jquery-1.11.3.min.js') }}"></script>
        <!---BootStrap.min.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/bootstrap.min.js') }}"></script>
        <!--HTML5 Js-->
        <script src="{{ asset('bundles/cilantromedia/site-assets/js/html5shiv.js') }}" type="text/javascript"></script>
        <!---Migrate.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/migrate.js') }}"></script>
        <!---Owl Carousel Slider.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/owl.carousel.min.js') }}"></script>
        <!---Bx Slider.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/jquery.bxslider.min.js') }}"></script>
        <!---Countdown.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/jquery.countdown.js') }}"></script>
        <!---PrettyPhoto.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/jquery.prettyPhoto.js') }}"></script>
        <!---Custom Script.js-->
        <script type="text/javascript" src="{{ asset('bundles/cilantromedia/site-assets/js/custom-script.js') }}"></script>

        <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-57962bf91e49c8dd"></script>

        {% if app.environment != 'dev' %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
            ga('create', 'UA-81169201-1', 'auto');
            ga('send', 'pageview');

            window.fbAsyncInit = function() {
                FB.init({
                    appId      : '1652024101779135',
                    xfbml      : true,
                    version    : 'v2.7'
                });
            };

            (function(d, s, id){
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) {return;}
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
        {% endif %}
    </body>
</html>